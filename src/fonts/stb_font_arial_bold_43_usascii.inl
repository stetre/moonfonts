        for (i=0; i < STB_FONT_arial_bold_43_usascii_NUM_CHARS; ++i) {
            // pad characters so they bilerp from empty space around each character
            font[i].s0 = (stb__arial_bold_43_usascii_s[i]) * recip_width;
            font[i].t0 = (stb__arial_bold_43_usascii_t[i]) * recip_height;
            font[i].s1 = (stb__arial_bold_43_usascii_s[i] + stb__arial_bold_43_usascii_w[i]) * recip_width;
            font[i].t1 = (stb__arial_bold_43_usascii_t[i] + stb__arial_bold_43_usascii_h[i]) * recip_height;
            font[i].x0 = stb__arial_bold_43_usascii_x[i];
            font[i].y0 = stb__arial_bold_43_usascii_y[i];
            font[i].x1 = stb__arial_bold_43_usascii_x[i] + stb__arial_bold_43_usascii_w[i];
            font[i].y1 = stb__arial_bold_43_usascii_y[i] + stb__arial_bold_43_usascii_h[i];
            font[i].advance_int = (stb__arial_bold_43_usascii_a[i]+8)>>4;
            font[i].s0f = (stb__arial_bold_43_usascii_s[i] - 0.5f) * recip_width;
            font[i].t0f = (stb__arial_bold_43_usascii_t[i] - 0.5f) * recip_height;
            font[i].s1f = (stb__arial_bold_43_usascii_s[i] + stb__arial_bold_43_usascii_w[i] + 0.5f) * recip_width;
            font[i].t1f = (stb__arial_bold_43_usascii_t[i] + stb__arial_bold_43_usascii_h[i] + 0.5f) * recip_height;
            font[i].x0f = stb__arial_bold_43_usascii_x[i] - 0.5f;
            font[i].y0f = stb__arial_bold_43_usascii_y[i] - 0.5f;
            font[i].x1f = stb__arial_bold_43_usascii_x[i] + stb__arial_bold_43_usascii_w[i] + 0.5f;
            font[i].y1f = stb__arial_bold_43_usascii_y[i] + stb__arial_bold_43_usascii_h[i] + 0.5f;
            font[i].advance = stb__arial_bold_43_usascii_a[i]/16.0f;
        }
    }
