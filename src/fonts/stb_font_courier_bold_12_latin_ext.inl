    if (font != 0) {
        float recip_width = 1.0f / STB_FONT_courier_bold_12_latin_ext_BITMAP_WIDTH;
        float recip_height = 1.0f / height;
        for (i=0; i < STB_FONT_courier_bold_12_latin_ext_NUM_CHARS; ++i) {
            // pad characters so they bilerp from empty space around each character
            font[i].s0 = (stb__courier_bold_12_latin_ext_s[i]) * recip_width;
            font[i].t0 = (stb__courier_bold_12_latin_ext_t[i]) * recip_height;
            font[i].s1 = (stb__courier_bold_12_latin_ext_s[i] + stb__courier_bold_12_latin_ext_w[i]) * recip_width;
            font[i].t1 = (stb__courier_bold_12_latin_ext_t[i] + stb__courier_bold_12_latin_ext_h[i]) * recip_height;
            font[i].x0 = stb__courier_bold_12_latin_ext_x[i];
            font[i].y0 = stb__courier_bold_12_latin_ext_y[i];
            font[i].x1 = stb__courier_bold_12_latin_ext_x[i] + stb__courier_bold_12_latin_ext_w[i];
            font[i].y1 = stb__courier_bold_12_latin_ext_y[i] + stb__courier_bold_12_latin_ext_h[i];
            font[i].advance_int = (stb__courier_bold_12_latin_ext_a[i]+8)>>4;
            font[i].s0f = (stb__courier_bold_12_latin_ext_s[i] - 0.5f) * recip_width;
            font[i].t0f = (stb__courier_bold_12_latin_ext_t[i] - 0.5f) * recip_height;
            font[i].s1f = (stb__courier_bold_12_latin_ext_s[i] + stb__courier_bold_12_latin_ext_w[i] + 0.5f) * recip_width;
            font[i].t1f = (stb__courier_bold_12_latin_ext_t[i] + stb__courier_bold_12_latin_ext_h[i] + 0.5f) * recip_height;
            font[i].x0f = stb__courier_bold_12_latin_ext_x[i] - 0.5f;
            font[i].y0f = stb__courier_bold_12_latin_ext_y[i] - 0.5f;
            font[i].x1f = stb__courier_bold_12_latin_ext_x[i] + stb__courier_bold_12_latin_ext_w[i] + 0.5f;
            font[i].y1f = stb__courier_bold_12_latin_ext_y[i] + stb__courier_bold_12_latin_ext_h[i] + 0.5f;
            font[i].advance = stb__courier_bold_12_latin_ext_a[i]/16.0f;
        }
    }
}

